@model WebStore.Models.EmployeeView
@{
    ViewData["Title"] = "Details";
}



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" charset="utf-8" />
    <h1>Подробная информация о сотруднике  @Model.SurName </h1>
</head>
<body>
    <table>
        <tr><td>Фамилия:</td><td>@Model.SurName</td></tr>
        <tr><td>Имя:</td><td>@Model.FirstName</td></tr>
        <tr><td>Отчество:</td><td>@Model.Patronymic</td></tr>
        <tr><td>Возраст:</td><td>@Model.Age</td></tr>
    </table>
    @Html.ActionLink("Назад", "Index")
</body>
</html>